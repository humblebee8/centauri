---
import Default from "$layouts/Default.astro";
import Signin from "$components/supabase/signin.svelte";
---
<Default title="login" description="join our community">
	<div class="bg-centauri min-height-400">
		<div class="p-6">
			<h1 class="text-center text-4xl uppercase font-bold">Login</h1>
            <Signin client:load></Signin>
		</div>
	</div>
</Default>

<style>
  body {
    @apply bg-astro text-gray-300;
  }
</style>